<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="580.0" prefWidth="960.0" style="-fx-background-color: ffbbbb;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="infoshop_project.FXML4ExitController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="500.0" layoutX="10.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/Logo%20Infoshop.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="562.0" layoutY="188.0" text="Are you sure want to exit?">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button layoutX="563.0" layoutY="284.0" mnemonicParsing="false" onAction="#yesButtonAction" style="-fx-background-color: ffecec;" text="Yes, I have Finished my work">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="574.0" layoutY="220.0" text="*All unsaved data will be lost">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="563.0" layoutY="337.0" mnemonicParsing="false" onAction="#noButtonAction" style="-fx-background-color: ffecec;" text="No, Let me back to work">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
